{
  "name": "@google-genkit/providers",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "./lib/cjs/llms.js",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean compile",
    "build:watch": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json --watch",
    "lint": "eslint --config ../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.10.0",
    "@google-cloud/vertexai": "file:google-cloud-vertexai-0.2.1.tgz",
    "@google-genkit/ai": "*",
    "@google-genkit/common": "*",
    "@pinecone-database/pinecone": "^2.0.1",
    "chromadb": "^1.7.3",
    "compute-cosine-similarity": "^1.1.0",
    "openai": "^4.24.2",
    "ts-md5": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.0"
  },
  "types": "lib/types/index.d.ts",
  "exports": {
    "./vectorstores": {
      "types": "./lib/types/vectorstores.d.ts",
      "require": "./lib/cjs/vectorstores.js",
      "import": "./lib/esm/vectorstores.js",
      "default": "./lib/esm/vectorstores.js"
    },
    "./llms": {
      "types": "./lib/types/llms.d.ts",
      "require": "./lib/cjs/llms.js",
      "import": "./lib/esm/llms.js",
      "default": "./lib/esm/llms.js"
    },
    "./embedders": {
      "types": "./lib/types/embedders.d.ts",
      "require": "./lib/cjs/embedders.js",
      "import": "./lib/esm/embedders.js",
      "default": "./lib/esm/embedders.js"
    },
    "./models": {
      "types": "./lib/types/models.d.ts",
      "require": "./lib/cjs/models.js",
      "import": "./lib/esm/models.js",
      "default": "./lib/esm/models.js"
    }
  },
  "typesVersions": {
    "*": {
      "vectorstores": [
        "lib/types/vectorstores"
      ],
      "llms": [
        "lib/types/llms"
      ],
      "embedders": [
        "lib/types/embedders"
      ],
      "models": [
        "lib/types/models"
      ]
    }
  }
}
